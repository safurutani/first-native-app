{
  "expo": {
    "name": "WordSmith",
    "slug": "WordSmith",
    "version": "1.2.3",
    "versionCode": 6,
    "orientation": "portrait",
    "icon": "./assets/images/wordsmith app icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "displayName": "WordSmith",
    "splash": {
      "image": "./assets/images/wordsmith app icon.png", 
      "resizeMode": "contain",
      "backgroundColor": "#006B61"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "versionCode": 6,
      "package": "com.bluesara24.wordsmith",
      "userInterfaceStyle": "light"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wordsmith app icon.png",
        "backgroundColor": "#006B61"
      },
      "versionCode": 6,
      "package": "com.bluesara24.wordsmith",
      "userInterfaceStyle": "light"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
